
<robot name="cinderella_3d">
  <material name="orange">
    <color rgba="1.0 0.5 0.2 1" />
  </material>
  <material name="gray">
    <color rgba="0.2 0.2 0.2 1" />
  </material>

  <link name="imu_link">
    <visual>
      <origin xyz="0.0 0.0 0.0" />
      <geometry>
        <box size="0.06 0.04 0.02" />
      </geometry>
      <material name="orange" />
    </visual>
  </link>

  <link name="laser_hector_frame">
    <visual>
      <origin xyz="0.0 0.0 0.0" />
      <geometry>
        <cylinder length="0.07" radius="0.05" />
      </geometry>
      <material name="gray" />
    </visual>
  </link>

  <link name="base_link" />

  <link name="base_link_vislam" />

  <joint name="imu_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="imu_link" />
    <origin xyz="0.15 0 0" rpy="0 0 0" />
  </joint>

  <joint name="lidar_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="laser_hector_frame" />
    <origin xyz="0. 0. 0." rpy="0. 3.141593 0.076" />
  </joint>

  <joint name="odom_link_joint" type="fixed">
    <parent link="base_link_vislam" />
    <child link="base_link" />
    <origin xyz="-0.15 0. 0." rpy="0. 0. 0." />
  </joint>

</robot>
